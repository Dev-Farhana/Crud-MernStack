<% for(let i=0; i < Users.length; i++) { %> 
    <tr>
        <td> <%= i+1 %>  </td>
        <td> <%= Users[i].name %> </td>
        <td> <%= Users[i].email %> </td>
        <td> <%= Users[i].gender %> </td>
        <td> <%= Users[i].status %> </td>
        <td> 
            <a href="/update-user?id=<%= Users[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient"> <i class="fa-solid fa-pencil-alt"></i> </span>
            </a>
            
            <a class="btn border-shadow delete"  data-id="<%= Users[i]._id  %>">
                <span class="text-gradient"> <i class="fa-solid fa-times"></i> </span>
            </a>
        </td>
        <!-- <%= Users %> -->
    </tr>

<% } %>